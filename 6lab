#include<stdio.h>
#define N 3

main()
{
	int F,j,i;
	float q;
	float S[N][N];
	F=0;
	for(i=0;i<N;i++)
	{
		for(j=0;j<N;j++)
			scanf("%f",&S[i][j]); 
	}
		for(i=0;i<N;i++)
	{
		for(j=0;j<N-2;j++)
		{
			if(S[i][j+1] / S[i][j] == S[i][j+2]/S[i][j+1])
			F=1;
			else
			F=0;
		}
		if (F==1)
		printf("%d - yes\n",i);
		else if(F==0)
		printf("%d - no\n");
	F=0;	
	}
	getchar();
	return 0;
}
