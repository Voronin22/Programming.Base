#include<stdio.h>
#define N 3
#define K 3
main()
{
	int F,j,i;
	int S[K][N];
	F=0;
	for(i=0;i<K;i++)
	{
		for(j=0;j<N;j++)
		{
			scanf("%d",S[i][j]);
		}
	}
		for(i=0;i<K;i++)
	{
		for(j=0;j<N;j++)
		{
			if((S[i][j+1]/S[i][j])==(S[i][j+2]/S[i][j+1]))
			F=1;
			else
			F=0;
		}
		if (F==1)
		printf("%d - yes\n",i);
		else
		printf("%d - no\n");
		F=0;
	}
	getchar();
	return 0;
}
