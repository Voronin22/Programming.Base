#include<stdio.h>
#include<malloc.h>
#include<stdlib.h>

main()
{
	int F,j,i;
	float *S,q;
	int N,K;
	printf("write razm N and K:\n");
	scanf("%d %d",&N,&K);
	F=0;
	S = (float*)malloc(N*K*sizeof(float));
	for(i=0;i<N;i++)
	{
		for(j=0;j < K;j++)
			scanf("%f",S*[i][j]); 
	}
		for(i=0;i<N;i++)
	{
		for(j=0;j<K-2;j++)
		{
			if(S*[i][j+1] / S*[i][j] != S*[i][j+2]/ S*[i][j+1])
			F=1;
			break;
		}
		if (F==0)
		printf("%d - yes\n",i);
		else if(F==1)
		printf("%d - no\n");
	F=0;	
	}
		free(S);
	getchar();
	return 0;
}
