#include<stdio.h>
#include<string.h>

int main(void)
{
	char s1[100];
	char s2[100];
	int i,F,chars,pos,id;
	printf("write a string:\n");
	scanf("%s",s1);
	chars=F=id=0;
	for(i=0;i<=99;i++)
	{
		if(s1[i]==' '|| s1[i]=='.'||s1[i]==','||s1[i]=='\t'||s1[i]=='\n')
		{   
  		    if(F==1)
  		       {
  		    	  if(((chars-pos)%2)==0)
  		    	  {
  		    	  	for(pos=i;pos<=(chars+1);pos++)
  		    	  	{
  		    	  		s2[id]=s1[pos];
  		    	  		id++;            
					}
				  }
				  F=0;
			   }
	        else 
	            {
	    	    if(F==0){
	    	       F=1;
	    	       pos=i;
				        }
		        if (F==1)
			       chars++;
		        }
	    }
   }
	printf("%s",s2);
	return 0;
}
